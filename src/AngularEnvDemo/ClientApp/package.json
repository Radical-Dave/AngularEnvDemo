{
  "name": "angularenvdemo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./src/assets/scripts/setEnv.ts",
    "prestart": "node aspnetcore-https",
    "start": "npm run config -- --environment=dev && run-script-os",
    "start:windows": "ng serve --port 44439 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44439 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "build": "npm run config -- --environment=prod && ng build --ENVIRONMENT=prod",
    "build:ssr": "ng run AngularEnvDemo:server:dev",
    "watch": "npm run config -- --environment=prod && ng build --watch --configuration prod --ENVIRONMENT=prod",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/platform-server": "~13.1.1",
    "@angular/router": "~13.1.1",
    "assert": "^2.0.0",
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0",
    "oidc-client": "^1.11.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "popper.js": "^1.16.0",
    "process": "^0.11.10",
    "run-script-os": "^1.1.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "util": "^0.12.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/build-angular": "^13.3.7",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.20.54",
    "dotenv": "^16.0.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.4.4",
    "yargs": "^17.5.1"
  }
}
